{
  "externalPattern": [
    "**/node_modules/**",
    "**/dist/**/*.ts"
  ],
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/leetcode/*"
  ],
  "packageOptions": {
    "entryPoints": [
      "src/common/index.ts"
    ]
  },
  "name": "Typescript-Algorithms",
  "out": "./docs/gh-pages/docs",
  // 导航条
  "navigationLinks": {
    "如何科学刷题": "/docs/gh-pages/docs",
    "LeetCode热题100": "",
    "面试经典150题": "https://leetcode.cn/studyplan/top-interview-150/",
    "Github": "https://github.com/gepick-ai/ts-algorithms"
  },
  "hideGenerator": true,
  "plugin": [
    "typedoc-github-theme"
  ],
  "theme": "typedoc-github-theme",
  "customCss": "docs/assets/custom.css",
  "logLevel": "Error",
  "includeHierarchySummary": false,
  "headings": {
    "readme": false,
    "document": false
  },
  "navigation": {
    "includeGroups": true,
    "includeCategories": true
  },
  "lang": "zh"
}
